{
    "version": 1001,
    "index_patterns": ["*persons*"],
    "order": 0,
    "settings": {
        "index": {
            "refresh_interval": "5s"
        }
    },
    "mappings": {
        "properties": {
            "@timestamp": {
                "type": "date"
            },
            "@version": {
                "type": "text",
                "fields": {
                    "keyword": {
                        "type": "keyword",
                        "ignore_above": 256
                    }
                }
            },
            "bibframe:date@authdates": {
                "type": "text",
                "fields": {
                    "keyword": {
                        "type": "keyword",
                        "ignore_above": 256
                    }
                }
            },
            "bibframe:identifier@person": {
                "type": "keyword"
            },
            "bibframe:name": {
                "type": "text"
            },
            "bibframe:name@eng": {
                "type": "text"
            },
            "bibframe:name@skt": {
                "type": "text"
            },
            "bibframe:note@person": {
                "type": "text"
            },
            "bibframe:variantname": {
                "type": "text"
            },
            "bibframe:variantname@eng": {
                "type": "text"
            },
            "bibframe:variantname@skt": {
                "type": "text"
            }
        }
    }
}
