{
    "name": "create-react-app-express",
    "version": "1.0.0",
    "scripts": {
        "server": "nodemon index.js",
        "deploy:serverindex": "scp -pr index.js root@acip-search:/var/www/server.api",
        "deploy:server": "scp -pr server root@acip-search:/var/www/server.api/",
        "deploy:queries": "scp -pr server/queries/* root@acip-search:/var/www/server.api/server/queries",
        "deploy:nginx": "scp /usr/local/etc/nginx/nginx.conf root@acip-search:/var/www/search.asianclassics.org",
        "deploy:pm2": "scp ecosystem.config.js root@acip-search:/var/www"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "body-parser": "^1.18.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.0.0",
        "elasticsearch": "^16.6.0",
        "express": "^4.16.4",
        "express-validator": "^6.1.1",
        "nodemon": "^1.19.1"
    },
    "devDependencies": {
        "prettier": "^2.1.2"
    }
}
